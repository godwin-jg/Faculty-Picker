<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>KEC</title>
    <base href="/" />
    <link rel="icon" type="image/png" sizes="16x16" href="https://placement.kongu.edu/assets/favicons/192.png">
    <link href="/style.css" rel="stylesheet">

</head>

<body>

    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="loader">
            <svg class="circular" viewBox="25 25 50 50">
                <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
            </svg>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->


    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper">


        <!--**********************************
            Header start
        ***********************************-->
        <%- include('header') %>
            <!--**********************************
            Header end ti-comment-alt
        ***********************************-->

            <!--**********************************
            Sidebar start
        ***********************************-->
            <%- include('slidbar') %>
                <!--**********************************
                            Sidebar end
                          ***********************************-->

                <!--**********************************
            Content body start
        ***********************************-->
                <div class="content-body">

                    <div class="row page-titles mx-0">
                        <div class="col p-md-0">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="javascript:void(0)">Dashboard</a></li>
                                <li class="breadcrumb-item active"><a href="javascript:void(0)">Add Course</a></li>
                            </ol>
                        </div>
                    </div>
                    <!-- row -->

                    <div class="container-fluid">
                        <div class="row justify-content-center">
                            <div class="col-lg-12">
                                <div class="card">

                                    <div class="card-body">
                                        <div class="form-validation">
                                            <h4 class="card-title">Add Course</h4>


                                            <form action="/api/courses/addcourse" method="post" class="form-valide">
                                                <div class="form-group row">
                                                    <label class="col-lg-3 col-form-label" for="val-CourceName">Course
                                                        Name
                                                        <span class="text-danger">*</span>
                                                    </label>
                                                    <div class="col-lg-6">
                                                        <input type="text" class="form-control" id="coursename"
                                                            name="coursename" placeholder="Enter a Course Name"
                                                            required>
                                                    </div>
                                                </div>

                                                <div class="form-group row">
                                                    <label class="col-lg-3 col-form-label" for="val-CourceCode">Course
                                                        Code
                                                        <span class="text-danger">*</span>
                                                    </label>
                                                    <div class="col-lg-6">
                                                        <input type="text" class="form-control" id="courcecode"
                                                            name="coursecode" placeholder="Enter a Course Code"
                                                            required>
                                                    </div>
                                                </div>

                                                <div class="form-group row">
                                                    <label class="col-lg-3 col-form-label" for="val-Credit">No of Credit
                                                        <span class="text-danger">*</span>
                                                    </label>
                                                    <div class="col-lg-6">
                                                        <input type="text" class="form-control" id="credit"
                                                            name="credit" placeholder="Enter a No of Credit" required>
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-lg-3 col-form-label" for="val-NoOfSlots">No of
                                                        Slots
                                                        <span class="text-danger">*</span>
                                                    </label>
                                                    <div class="col-lg-6">
                                                        <input type="text" class="form-control" id="slots" name="slots"
                                                            placeholder="Enter No of Slots" required>
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-lg-3 col-form-label" for="val-NoOfSlots">Batch
                                                        <span class="text-danger">*</span>
                                                    </label>
                                                    <div class="col-lg-6">
                                                        <input type="text" class="form-control" id="batch" name="batch"
                                                            placeholder="Example: 2021" required>
                                                    </div>
                                                </div>

                                                <div class="form-group row">
                                                    <label class="col-lg-3 col-form-label" for="val-">Select the Course
                                                        type <span class="text-danger">*</span>
                                                    </label>
                                                    <div class="col-lg-6">
                                                        <select class="form-control" id="coursetype" name="coursetype">
                                                            <option value="">Please select</option>
                                                            <option value="Type 1">Type 1</option>
                                                            <option value="Type 2">Type 2</option>
                                                            <option value="Type 3">Type 3</option>

                                                        </select>
                                                    </div>
                                                </div>

                                                <div class="form-group row">
                                                    <label class="col-lg-3 col-form-label" for="">Select the
                                                        Semester<span class="text-danger">*</span>
                                                    </label>
                                                    <div class="col-lg-6" style="margin-top: 10px;">
                                                        <label class="radio-inline mr-3">
                                                            <input type="radio" name="optradio" value="1st sem"
                                                                required> 1st sem</label>
                                                        <label class="radio-inline mr-3">
                                                            <input type="radio" name="optradio" value="2nd sem"> 2nd
                                                            sem</label>
                                                        <label class="radio-inline mr-3">
                                                            <input type="radio" name="optradio" value="3rd sem"> 3rd
                                                            sem</label>
                                                        <label class="radio-inline mr-3">
                                                            <input type="radio" name="optradio" value="4th sem"> 4th
                                                            sem</label>
                                                        <label class="radio-inline mr-3">
                                                            <input type="radio" name="optradio" value="5th sem"> 5th
                                                            sem</label>
                                                        <label class="radio-inline mr-3">
                                                            <input type="radio" name="optradio" value="6th sem"> 6th
                                                            sem</label>
                                                        <label class="radio-inline mr-3">
                                                            <input type="radio" name="optradio" value="7th sem"> 7th
                                                            sem</label>
                                                        <label class="radio-inline mr-3">
                                                            <input type="radio" name="optradio" value="8th sem"> 8th
                                                            sem</label>
                                                    </div>
                                                </div>



                                                <div class="form-group row">

                                                    <label class="col-lg-3 col-form-label">Add Courses to <span
                                                            class="text-danger">*</span>
                                                    </label>
                                                    <div class="col-lg-8" style="margin-top: 10px;">
                                                        <div class="form-group">
                                                            <div class="row">
                                                                <div class="col-lg-6">
                                                                    <div class="form-check mb-3">
                                                                        <label class="form-check-label">
                                                                            <input type="checkbox"
                                                                                class="form-check-input"
                                                                                value="B.E. Civil Engineering">B.E.
                                                                            Civil Engineering</label>
                                                                    </div>
                                                                    <div class="form-check mb-3">
                                                                        <label class="form-check-label">
                                                                            <input type="checkbox"
                                                                                class="form-check-input"
                                                                                value="B.E. Mechanical Engineering">B.E.
                                                                            Mechanical Engineering</label>
                                                                    </div>
                                                                    <div class="form-check mb-3">
                                                                        <label class="form-check-label">
                                                                            <input type="checkbox"
                                                                                class="form-check-input"
                                                                                value="B.E. Mechatronics Engineering">B.E.
                                                                            Mechatronics Engineering</label>
                                                                    </div>
                                                                    <div class="form-check mb-3">
                                                                        <label class="form-check-label">
                                                                            <input type="checkbox"
                                                                                class="form-check-input"
                                                                                value="B.E. AutoMobile Engineering">B.E.
                                                                            AutoMobile Engineering</label>
                                                                    </div>
                                                                    <div class="form-check mb-3">
                                                                        <label class="form-check-label">
                                                                            <input type="checkbox"
                                                                                class="form-check-input"
                                                                                value="B.E. Chemical Engineering">B.E.
                                                                            Chemical Engineering</label>
                                                                    </div>
                                                                    <div class="form-check mb-3">
                                                                        <label class="form-check-label">
                                                                            <input type="checkbox"
                                                                                class="form-check-input"
                                                                                value="B.Tech Food Technology">B.Tech
                                                                            Food Technology</label>
                                                                    </div>

                                                                    <div class="form-check mb-3">
                                                                        <label class="form-check-label">
                                                                            <input type="checkbox"
                                                                                class="form-check-input"
                                                                                value="B.E. Electrical and Electronics Engineering">B.E.
                                                                            Electrical and Electronics
                                                                            Engineering</label>
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-6" style="margin-top: 10px;">
                                                                    <div class="form-check mb-3">
                                                                        <label class="form-check-label">
                                                                            <input type="checkbox"
                                                                                class="form-check-input"
                                                                                value="B.E. Electronics and Communication Engineering">B.E.
                                                                            Electronics and Communication
                                                                            Engineering</label>
                                                                    </div>
                                                                    <div class="form-check mb-3">
                                                                        <label class="form-check-label">
                                                                            <input type="checkbox"
                                                                                class="form-check-input"
                                                                                value="B.E. Computer Science and Engineering">B.E.
                                                                            Computer Science and Engineering</label>
                                                                    </div>
                                                                    <div class="form-check mb-3">
                                                                        <label class="form-check-label">
                                                                            <input type="checkbox"
                                                                                class="form-check-input"
                                                                                value="B.Tech. Information Technology">B.Tech.
                                                                            Information Technology</label>
                                                                    </div>


                                                                    <div class="form-check mb-3">
                                                                        <label class="form-check-label">
                                                                            <input type="checkbox"
                                                                                class="form-check-input"
                                                                                value="B.E. Computer Science and Design">B.E.
                                                                            Computer Science and Design</label>
                                                                    </div>
                                                                    <div class="form-check mb-3">
                                                                        <label class="form-check-label">
                                                                            <input type="checkbox"
                                                                                class="form-check-input"
                                                                                value="B.Tech Artificial Intelligence and Data Science">B.Tech
                                                                            Artificial Intelligence and Data
                                                                            Science</label>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <div class="col-lg-8 ml-auto">
                                                        <button class="btn btn-primary">Add Course</button>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- #/ container -->
                </div>
                <!--**********************************
            Content body end
        ***********************************-->

                <!--**********************************
            Footer start
        ***********************************-->
                <%- include('footer') %>
                    <!--**********************************
            Footer end
        ***********************************-->
    </div>
    <!--**********************************
        Main wrapper end
    ***********************************-->

    <!--**********************************
        Scripts
    ***********************************-->
    <script src="plugins/common/common.min.js"></script>
    <script src="js/custom.min.js"></script>
    <script src="js/settings.js"></script>
    <script src="js/gleek.js"></script>
    <script src="js/styleSwitcher.js"></script>

    <script src="./plugins/validation/jquery.validate.min.js"></script>
    <script src="./plugins/validation/jquery.validate-init.js"></script>

</body>

</html>